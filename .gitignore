# vim: set ft=gitignore:

########
# Fish
####
# Guard against generated completion scripts
fish/.config/fish/completions/
fish/.config/fish/fish_variables
fish/.config/fish/fishd.*

########
# Git
####
# git-identity config
git/.config/git/x-identity.conf

########
# SSH
####
ssh/.ssh/hosts.d/
ssh/.ssh/id_*
ssh/.ssh/keys/
ssh/.ssh/known_hosts*

########
# Shell
####
# Local includes
shell/.config/shell/includes.d/*.sh

# litecli, pgcli, etc
shell/.config/*cli/history
shell/.config/*cli/log

########
# Vim
####
vim/.config/*/local/*.vim
vim/.config/nvim/*
